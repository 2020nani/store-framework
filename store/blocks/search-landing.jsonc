{
    "store.custom#landing": { 
      "blocks": [
        "image#landingbanner", 
        "search-result-layout.customQuery"
      ]
    },
    "search-result-layout.customQuery":{
        "props":{
            "querySchema": {
                "orderByField": "OrderByScoreASC",
                "maxItemsPerPage": 8,
                "hideUnavailableItems": true,
                "simulationBehavior": "skip"
              }
        }
    },
    "image#landingbanner":{
        "props":{
            "src": "https://storecomponents.vteximg.com.br/arquivos/box.png",
            "minWidth": 100
        }
    }
  }